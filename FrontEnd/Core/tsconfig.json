{
  "buildOptions": {
    "incremental": true
  },
  "compilerOptions": {
    "allowJs": false,
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "dist",
    "paths": {
      "@Core/*": [
        "./*"
      ]
    },
    "strict": true,
    "target": "ESNext"
  },
}
